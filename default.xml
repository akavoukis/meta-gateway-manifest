<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <default sync-j="4" revision="rocko"/>

  <remote fetch="https://git.yoctoproject.org/git"	name="yocto"/>
  <remote fetch="https://github.com/agherzan" 		name="raspberrypi"/>
  <remote fetch="https://github.com/sbabic"		name="swupdate"/>
  <remote fetch="https://github.com/akavoukis"          name="gateway" revision="master"/>
  <remote fetch="https://github.com/openembedded"  	name="oe"/>

  <project remote="yocto"		upstream="rocko"  name="poky"/>
  <project remote="raspberrypi"		upstream="rocko"  name="meta-raspberrypi" 	path="poky/meta-raspberrypi"/>
  <project remote="swupdate"		upstream="rocko"  name="meta-swupdate" 		path="poky/meta-swupdate"/>
  <project remote="yocto"               upstream="rocko"  name="meta-mono" 		path="poky/meta-mono"/>
  <project remote="gateway"             upstream="master" name="meta-gateway" 		path="poky/meta-gateway"/>
  <project remote="oe"                  upstream="rocko"  name="meta-openembedded"      path="poky/meta-openembedded"/>

</manifest>
